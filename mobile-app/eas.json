{
  "cli": {
    "version": ">= 16.6.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    },
    "local": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "env": {
        "API_URL": "http://192.168.1.10:3033"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
